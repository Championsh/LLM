```C++

using namespace std;
class Vector {
private:
   int x;
   int y;
};

struct Point {
 private:
     int x;
     int y;
     
public:
    struct Point(int x, int y){
         this->x = y;
         this->y = x;
    };
};

bool g(C* c)
{
    if(!c)return false;
    auto v = &c->v;
    Point pt;
    try
    {
       pt = ConvertToPoint(*v); //this line is not valid because of missing implementation for ConvertToPoint function
    }
    catch(...)
    {
        cout<<"Failed to convert vector to point";
        return false;
    }

    pt.x *= 2;
    pt.y *= 2;

    v->x = 5;
    v->y = 6;

    return true;
}

class C {
public:
    Vector v;
};

```